{
  "env": { "browser": true, "node": true, "es2021": true },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "prefer-template": "off",
    "quotes": ["error", "double"],
    "react/prop-types": "off"
  }
}
